# Keep connections open in the background.
# Close idle connections after 5 minutes.
Host *
  Compression yes
  ControlMaster auto
  ControlPath ~/.ssh/control/%h_%p_%r
  ControlPersist 300
#  UseRoaming no
  UseKeychain yes
  ServerAliveInterval 30
  ServerAliveCountMax 3
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_rsa

# Work
Host *.execvision.io
  User mconley
  ForwardAgent true
  IdentitiesOnly true
  IdentityFile ~/.ssh/id_rsa_ev
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null 
Host 10.0.*.*
  User mconley
  ForwardAgent true
  IdentitiesOnly true
  IdentityFile ~/.ssh/id_rsa_ev
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null 
